<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
<bean id="triangle" class="cab.springdemo10.Triangle" autowire="byName" />
	
<bean id="pointA" class="cab.springdemo10.Point" >
	<property name="x" value="0" />
	<property name="y" value="0" />
</bean>

<bean id="pointB" class="cab.springdemo10.Point" >
	<property name="x" value="-20" />
	<property name="y" value="0" />
</bean>

<bean id="pointC" class="cab.springdemo10.Point" >
	<property name="x" value="0" />
	<property name="y" value="20" />
</bean>

</beans>