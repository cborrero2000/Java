<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<context:component-scan base-package="cab.springdemo21"></context:component-scan>
	<context:annotation-config />

	<bean id="pointA" class="cab.springdemo21.Point">
		<qualifier value="circleRelated" />
		<property name="x" value="5" />
		<property name="y" value="12" />
	</bean>

	<bean id="pointB" class="cab.springdemo21.Point">
		<property name="x" value="0" />
		<property name="y" value="0" />
	</bean>

	<bean id="center" class="cab.springdemo21.Point">
		<property name="x" value="10" />
		<property name="y" value="10" />
	</bean>

</beans>